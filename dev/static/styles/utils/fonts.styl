/* source-sans-pro-200 - cyrillic_latin */
@font-face {
  font-family: 'source_sans_proextralight';
  font-style: normal;
  font-weight: 200;
  src: url('../fonts/source-sans-pro-v12-cyrillic_latin-200.eot'); /* IE9 Compat Modes */
  src: local('Source Sans Pro ExtraLight'), local('SourceSansPro-ExtraLight'),
       url('../fonts/source-sans-pro-v12-cyrillic_latin-200.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
       url('../fonts/source-sans-pro-v12-cyrillic_latin-200.woff2') format('woff2'), /* Super Modern Browsers */
       url('../fonts/source-sans-pro-v12-cyrillic_latin-200.woff') format('woff'), /* Modern Browsers */
       url('../fonts/source-sans-pro-v12-cyrillic_latin-200.ttf') format('truetype'), /* Safari, Android, iOS */
       url('../fonts/source-sans-pro-v12-cyrillic_latin-200.svg#SourceSansPro') format('svg'); /* Legacy iOS */
}
